target C {
    timeout: 5 sec,
    coordination: decentralized
}
import Count, Print from "Federated.lf"
reactor PrintTimer extends Print {
    timer t(0, 1 sec);
    reaction(t) {=
        lf_print("Timer ticked at (%lld, %d).", 
            lf_time_logical_elapsed(), lf_tag().microstep
        );
    =}
}
federated reactor {
    c = new Count();
    p = new PrintTimer();
    c.out -> p.in;
}
