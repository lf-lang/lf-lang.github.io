target Cpp {
  timeout: 3 s
}

main reactor Alignment {
  state s: int(0)
  timer t1(100 ms, 100 ms)
  timer t2(200 ms, 200 ms)
  timer t4(400 ms, 400 ms)

  reaction(t1) {=
    s += 1;
  =}

  reaction(t2) {=
    s -= 2;
  =}

  reaction(t4) {=
    std::cout << "s = " << std::to_string(s) << std::endl;
  =}
}
