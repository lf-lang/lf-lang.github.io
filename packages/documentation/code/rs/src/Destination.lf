target Rust;
reactor Destination {
    input x:u32;
    input y:u32;
    reaction(x, y) {=
        let mut sum = 0;
        if let Some(x) = ctx.get(x) {
            sum += x;
        }
        if let Some(y) = ctx.get(y) {
            sum += y;
        }
        println!("Received {}.", sum);
    =}
}