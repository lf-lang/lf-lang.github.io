target TypeScript;
reactor Source {
    output[3] out:number;
    reaction(startup) -> out {=
         for (let i = 0 ; i < out.length; i++) {
            out[i] = i;
        }
    =}
}
reactor Destination {
    input inp:number;
    reaction(inp) {=
        console.log(`Destination ${this.getBankIndex()} received ${inp}`)
    =}
}

main reactor MultiportToBank {
    a = new Source();
    b = new[3] Destination();
    a.out -> b.inp;
}